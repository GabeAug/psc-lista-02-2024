programa {
  funcao inicio() {
    inteiro n1, n2, soma, subtracao, multiplicacao

    escreva("Digite seu primeiro número: ")
    leia(n1)
    escreva("Digite seu segundo número: ")
    leia(n2)
    soma = n1 + n2
    subtracao = n1 - n2
    multiplicacao = n1 * n2
    escreva("Soma: " + soma + "\nSubtração: " + subtracao + "\nMultiplicação: " + multiplicacao)
  }
}
